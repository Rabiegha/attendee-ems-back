{
  "name": "attendee-ems-back",
  "version": "1.0.0",
  "description": "Multi-tenant EMS backend with NestJS",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:seed:orgs": "ts-node scripts/seed-specific.ts organizations",
    "db:seed:users": "ts-node scripts/seed-specific.ts users",
    "db:seed:minimal": "ts-node scripts/seed-specific.ts minimal",
    "db:seed:event": "ts-node prisma/seeders/seed-specific-event.ts",
    "db:studio": "prisma studio",
    "db:migrate:deploy": "dotenv -e .env.prod -- prisma migrate deploy",
    "db:seed:deploy": "dotenv -e .env.prod -- prisma db seed",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "db:seed:test": "dotenv -e .env.test -- prisma db seed",
    "docker:seed": "docker-compose -f docker-compose.dev.yml exec api npm run db:seed",
    "docker:migrate": "docker-compose -f docker-compose.dev.yml exec api npm run db:migrate",
    "docker:migrate:reset": "docker-compose -f docker-compose.dev.yml exec api npm run db:migrate:reset",
    "docker:migrate:deploy": "docker-compose -f docker-compose.dev.yml exec api npx prisma migrate deploy",
    "docker:generate": "docker-compose -f docker-compose.dev.yml exec api npx prisma generate",
    "docker:studio": "docker-compose -f docker-compose.dev.yml exec api npm run db:studio",
    "docker:db:push": "docker-compose -f docker-compose.dev.yml exec api npm run db:push",
    "docker:db:reset": "docker-compose -f docker-compose.dev.yml exec api npx prisma migrate reset --force",
    "docker:db:status": "docker-compose -f docker-compose.dev.yml exec api npx prisma migrate status",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f api",
    "docker:logs:db": "docker-compose -f docker-compose.dev.yml logs -f db",
    "docker:shell": "docker-compose -f docker-compose.dev.yml exec api bash",
    "docker:shell:db": "docker-compose -f docker-compose.dev.yml exec db psql -U postgres -d ems",
    "docker:install": "docker-compose -f docker-compose.dev.yml exec api npm i",
    "docker:restart": "docker-compose -f docker-compose.dev.yml restart api"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^6.17.1",
    "@types/nodemailer": "^7.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^17.2.2",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.15.13",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.17.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
